#. module: inventory_log
#: model:mail.template,body_html:inventory_log.quality_not_checked_email_template_v1_2
msgid ""
"<div style=\"line-height: 1.6;\">\n"
"                    <span>It has been detected that the following picking "
"validated by ${ctx.get('user')} does not pass the quality control at ${ctx."
"get('datetime')}.</span>\n"
"                    <li><span style=\"font-weight: bolder;\">Picking: </"
"span> ${object.picking_id.name}</li>\n"
"                    <li><span style=\"font-weight: bolder;\">Affected move "
"lines: </span>\n"
"                    <ul>\n"
"                    % for line in object.quality_line_ids:\n"
"                        <li> \n"
"                            <span>${line.product_id.name}</span>\n"
"                        </li>\n"
"                        <ul>\n"
"                        <li>\n"
"                            <span style=\"font-weight: bolder;\">Lot: </"
"span>\n"
"                            ${line.lot_id.name}\n"
"                        </li>\n"
"                        <li>\n"
"                            <span style=\"font-weight: bolder;\">Qty: </"
"span>\n"
"                            ${line.qty_done}\n"
"                        </li>\n"
"                        % if line.failed_expiration:\n"
"                            <li>\n"
"                                <span style=\"font-weight: bolder;\">Wrong "
"expiration</span>\n"
"                            </li>\n"
"                        % endif\n"
"                        % if not line.correct_state_of_product:\n"
"                            <li>\n"
"                                <span style=\"font-weight: bolder;\">Reason:"
"</span>\n"
"                                ${line.reason_state}\n"
"                            </li>\n"
"                        % endif\n"
"                        </ul>\n"
"                    % endfor\n"
"                    </ul>\n"
"                    </li>\n"
"                    <li><span style=\"font-weight: bolder;\">Temperature: </"
"span>\n"
"                    % if object.temperature:\n"
"                        Correct\n"
"                    % else:\n"
"                        Not correct\n"
"                    % endif\n"
"                     </li>\n"
"                    <li><span style=\"font-weight: bolder;\">State of the "
"van: </span>\n"
"                    % if object.state_of_the_van:\n"
"                        Correct\n"
"                    % else:\n"
"                        Not correct\n"
"                    % endif \n"
"                    </li>\n"
"                    <li><span style=\"font-weight: bolder;\">Message: </"
"span> ${ctx.get('reason')}</li>\n"
"                </div>\n"
"            "
msgstr ""
"<div style=\"line-height: 1.6;\">\n"
"                    <span>Se ha detectado la validación de una transferencia "
"generada por ${ctx.get('user')} que no ha pasada el control de calidad con "
"fecha ${ctx.get('datetime')}.</span>\n"
"                    <li><span style=\"font-weight: bolder;\">Transferencia: "
"</span> ${object.picking_id.name}</li>\n"
"                    <li><span style=\"font-weight: bolder;\">Lineas "
"afectadas: </span>\n"
"                    <ul>\n"
"                    % for line in object.quality_line_ids:\n"
"                        <li> \n"
"                            <span>${line.product_id.name}</span>\n"
"                        </li>\n"
"                        <ul>\n"
"                        <li>\n"
"                            <span style=\"font-weight: bolder;\">Lote: </"
"span>\n"
"                            ${line.lot_id.name}\n"
"                        </li>\n"
"                        <li>\n"
"                            <span style=\"font-weight: bolder;\">cantidad: </"
"span>\n"
"                            ${line.qty_done}\n"
"                        </li>\n"
"                        % if line.failed_expiration:\n"
"                            <li>\n"
"                                <span style=\"font-weight: bolder;"
"\">Expiración incorrecta</span>\n"
"                            </li>\n"
"                        % endif\n"
"                        % if not line.correct_state_of_product:\n"
"                            <li>\n"
"                                <span style=\"font-weight: bolder;\">Razón:</"
"span>\n"
"                                ${line.reason_state}\n"
"                            </li>\n"
"                        % endif\n"
"                        </ul>\n"
"                    % endfor\n"
"                    </ul>\n"
"                    </li>\n"
"                    <li><span style=\"font-weight: bolder;\">Temperatura: </"
"span>\n"
"                    % if object.temperature:\n"
"                        Correct\n"
"                    % else:\n"
"                        No correcta\n"
"                    % endif\n"
"                     </li>\n"
"                    <li><span style=\"font-weight: bolder;\">Estado del "
"camión: </span>\n"
"                    % if object.state_of_the_van:\n"
"                        Correcto\n"
"                    % else:\n"
"                        incorrecto\n"
"                    % endif \n"
"                    </li>\n"
"                    <li><span style=\"font-weight: bolder;\">Mensaje: </"
"span> ${ctx.get('reason')}</li>\n"
"                </div>\n"
"            "

